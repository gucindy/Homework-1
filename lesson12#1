var number1, number2 : int
    put "Enter 2 even numbers."
    get number1, number2
    if number1 > number2 then
        for decreasing j : number1 .. number2 by 2
            put j, ", " ..
        end for
    else
        for i : number1 .. number2 by 2
            put i, ", " ..
        end for
    end if
